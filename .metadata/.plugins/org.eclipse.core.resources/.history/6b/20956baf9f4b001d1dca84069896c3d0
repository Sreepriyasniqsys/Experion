public class Assignment {

	public static void main(String[] args) {
		String name = "Sreepriya";
		int age = 23;
		char gender = 'F';
		double cgpa = 8.4;
		System.out.println("*******Welcome to ILP Program*********");
		System.out.println("Name" +  "Age" + "Gender" + "CGPA");
		System.out.println(name + age + gender + cgpa);

	}

}
